{
  "name": "warsworld",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^21.0.0"
  },
  "type": "module",
  "scripts": {
    "db:up": "docker compose up -d",
    "db:destroy": "docker compose down --volumes --remove-orphans",
    "prisma:push": "prisma db push",
    "prisma:seed": "prisma db seed",
    "prisma:reset": "prisma migrate reset",
    "prisma:generate-once": "prisma generate",
    "prisma:generate-watch": "prisma generate --watch",
    "prisma:studio": "prisma studio",
    "dev:wss": "cross-env PORT=3001 tsx watch src/server/main-wss-development.ts --tsconfig tsconfig.server.json",
    "dev:next": "next dev",
    "dev": "npm run prisma:push && concurrently \"npm:dev:*\"",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint . --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:fix": "npm run lint:prettier:fix && npm run lint:eslint:fix",
    "build:next": "cross-env NODE_ENV=production next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "concurrently \"npm:build:*\"",
    "start": "cross-env NODE_ENV=production node dist/server/main-production.js",
    "redundant-code": "npx jscpd --ignore node_modules,dist,public,.next --reporters consoleFull .",
    "update-depencencies": "npx npm-check-updates -i"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "singleQuote": false,
    "endOfLine": "auto",
    "printWidth": 100
  },
  "dependencies": {
    "@auth/core": "^0.24.0",
    "@headlessui/react": "^1.7.18",
    "@prisma/client": "^5.8.1",
    "@react-hook/window-size": "^3.1.1",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-table": "^8.11.7",
    "@trpc/client": "^10.45.0",
    "@trpc/next": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "bcrypt": "^5.1.1",
    "gray-matter": "^4.0.3",
    "next": "^14.1.0",
    "next-auth": "^4.24.5",
    "pixi.js": "^7.4.0",
    "prisma-json-types-generator": "^3.0.3",
    "react": "^18.2.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "sass": "^1.70.0",
    "superjson": "^2.2.1",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.0",
    "@stylistic/eslint-plugin-ts": "^1.5.4",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.10",
    "@types/react": "^18.2.48",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "prisma": "^5.8.1",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
