{
  "name": "warsworld",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^18.15.0"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "initialise": "npm run db:up && prisma db push && prisma db seed",
    "db:up": "docker-compose up -d",
    "db:destroy": "docker-compose down --volumes --remove-orphans",
    "prisma:push": "prisma db push",
    "prisma:seed": "prisma db seed",
    "prisma:reset": "prisma migrate reset",
    "prisma:generate-once": "prisma generate",
    "prisma:generate-watch": "prisma generate --watch",
    "prisma:studio": "prisma studio",
    "dev:wss": "cross-env PORT=3001 tsx watch src/server/main-wss-development.ts --tsconfig tsconfig.server.json ",
    "dev:next": "next dev",
    "dev": "npm run prisma:push && concurrently \"npm:dev:*\"",
    "lint": "eslint src --ext \".js,.ts,.tsx\" --cache --report-unused-disable-directives --max-warnings=10",
    "lint:fix": "npm run lint --fix",
    "build:next": "cross-env NODE_ENV=production next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "concurrently \"npm:build:*\"",
    "start": "cross-env NODE_ENV=production node dist/server/main-production.js"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "singleQuote": false,
    "endOfLine": "auto"
  },
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "@trpc/client": "^10.19.1",
    "@trpc/next": "^10.19.1",
    "@trpc/server": "^10.19.1",
    "next": "^13.2.1",
    "next-auth": "^4.14.0",
    "pixijs": "^7.1.4",
    "prisma-json-types-generator": "^2.3.1",
    "react": "^18.2.0",
    "sass": "^1.62.0",
    "superjson": "^1.7.4",
    "ws": "^8.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.20",
    "@types/react": "^18.0.9",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.2.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prisma": "^4.12.0",
    "tailwindcss": "^3.3.0",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  }
}
