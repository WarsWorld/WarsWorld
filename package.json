{
  "name": "warsworld",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^18.15.0"
  },
  "packageManager": "pnpm@7.27.1",
  "scripts": {
    "prebuild": "prisma generate && prisma migrate deploy",
    "build:1-next": "cross-env NODE_ENV=production next build",
    "build:2-server": "tsc --project tsconfig.server.json",
    "build": "run-s build:*",
    "dev:wss": "cross-env PORT=3001 tsx watch src/server/main-wss-development.ts --tsconfig tsconfig.server.json ",
    "dev:next": "next dev",
    "dev": "pnpm migrate-dev && run-p dev:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "prisma db push",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dx": "run-s db-up db-migrate-dev dev",
    "generate": "prisma generate",
    "migrate-dev": "prisma db push",
    "migrate": "prisma migrate deploy",
    "start": "cross-env NODE_ENV=production node dist/server/main-production.js",
    "studio": "prisma studio",
    "lint:eslint": "eslint src --ext \".js,.ts,.tsx\" --cache --report-unused-disable-directives --max-warnings=10",
    "lint:eslint:fix": "pnpm lint:eslint --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint": "pnpm run lint:prettier && pnpm run lint:eslint",
    "lint-fix": "pnpm run lint:prettier:fix && pnpm run lint:eslint:fix",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "singleQuote": false,
    "endOfLine": "auto"
  },
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.19.1",
    "@trpc/next": "^10.19.1",
    "@trpc/react-query": "^10.19.1",
    "@trpc/server": "^10.19.1",
    "clsx": "^1.1.1",
    "next": "^13.2.1",
    "next-auth": "^4.14.0",
    "pixijs": "^7.1.4",
    "prisma-json-types-generator": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.0",
    "superjson": "^1.7.4",
    "tinyqueue": "^2.0.3",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.12.3",
    "ws": "^8.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^4.3.8",
    "@types/node": "^18.7.20",
    "@types/react": "^18.0.9",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^4.9.5"
  }
}
